{
  "name": "Macro Scheduler",
  "version": "0.6",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
